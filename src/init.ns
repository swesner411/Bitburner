export async function main(ns) {
	let hostname = ns.getHostname()
	if (hostname !== 'home') {
		throw new Exception('Run this script from home')
	}
	
	ns.run('downloadFiles.ns',1);
	while(true) {
		if(ns.isRunning('downloadFiles.ns',hostname) == false) {break}
		await ns.sleep(5);
	}
	ns.run('rootAccess.ns', 1);
	while(true) {
		if(ns.isRunning('rootAccess.ns',hostname) == false) {break}
		await ns.sleep(5);
	}
	ns.run('transferFiles.ns', 1);
}
