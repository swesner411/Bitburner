const scriptsToKill = [
  'execNodes.ns',
  'node.ns',
  'killAll.ns',
]

export async function main(ns) {
	let hostname = ns.getHostname()
	if (hostname !== 'home') {
		throw new Exception('Run the script from home')
	}
	for(let i = 0; i < scriptsToKill.length; i++) {
		ns.killall(scriptsToKill[i])
	}
}