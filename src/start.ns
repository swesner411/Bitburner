const repo = "https://raw.githubusercontent.com/swesner411/bitburner/dev/src/"

export async function main(ns) {
  if (ns.getHostname() !== "home") {
    throw new Exception("Run this script from home");
  }
  await ns.wget(repo + "init.ns", "init.ns");
  await ns.wget(repo + "settings.ns", "settings.ns)
  ns.run("init.ns");
}
